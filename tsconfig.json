{
  "compilerOptions": {
    "sourceMap": true,
    "outDir": "lib",
    "declaration": true,
    "module": "CommonJS",
    "strict": true,
    "lib": ["es2018", "esnext.asynciterable"],
    "esModuleInterop": true,
    "moduleResolution": "node",
    "emitDecoratorMetadata": true,
    "experimentalDecorators": true,
    "target": "es5",
    "resolveJsonModule": true
  },
  "include": ["src"]
}
